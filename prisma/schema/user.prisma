model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  tags          Tag[]
  emailCodes    EmailCode[]
  passwordCodes PasswordCode[]
  schedule      Schedule[]

  @@index([email, id, username])
}
